
public Test :() ==> seq of ( nat | real )
Test () ==
    return 
    [ XF(1,1,3,3), XG(3,5),
      XH(3,3), XH(3.3,3.3), 
      Xf2(1,mk_(1,2)), Xg2(1), Xh2(mk_(1,2), mk_(2,1)), 
      aXQ(), aXQQ()] ;

  XF: nat*nat*nat*nat ==> nat
  XF(a,a,b,b) ==
    return a+b;
  
  XG: nat*nat ==> nat
  XG(a,b) ==
    return a + b;

  XH:(nat|real)*(nat|real) ==> (nat|real)
  XH(a,a) ==
    return a+3;

  Xf2:nat*(nat*nat) ==> nat
  Xf2(a, mk_(a,b)) ==
   return a*b;

  Xg2: nat ==> nat
  Xg2(a) ==
    return let a = a+1 in a ;

  Xh2: (nat*nat)*(nat*nat) ==> nat 
  Xh2(mk_(a,b),mk_(b,a) )==
    return a+b+XH(1,1);

aXQ: () ==> nat
aXQ() ==
  return 3;
  
aXQQ: () ==> nat
aXQQ() ==
  return if 1=1 then 4 else 5


end A
