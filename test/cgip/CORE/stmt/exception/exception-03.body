public Test : () ==> int | seq of int 
Test() ==
  Example();

DoCommand: () ==> seq of int
DoCommand() ==
( dcl l : seq of nat := [];
  always TestSeq(l) in
  ( l := SetToSeq({-2,...,8});
    return l )
);

SetToSeq: set of int ==> seq of int
SetToSeq(s) ==
  if s = {} then
    exit <ERR>
  else
    let e in set s in 
      return [e] ^ SetToSeq(s \ {e});

TestSeq: seq of int ==> seq of int
TestSeq(l) ==
  if l = [] then
    exit <ERR>
  else
    return l;

Example: () ==> int | seq of int
Example() ==
  tixe { 
    <ERR> |-> exit 7,
    err |-> return err
  } in DoCommand()

end A