  Test() ==
    return evalExpr();
   operations

  getState: () ==> B
  getState() == return state
  pre state <> nil;

  evalExpr: () ==> [ItemId] 
  evalExpr() ==
   (cases a:
    2 -> let x=1 in
         getState().getVarVal(),
    mk_(x,b) -> newNumVal(a),
    others->let y=1 in
         getState().getVarVal()
    end
    );
 
newObj: () ==> C
newObj() ==
(
 let s: A = new A().initSimObj()
  in return s
);

newNumVal : real ==> ItemId
newNumVal(v) ==
(
 getState().getVarVal()
);

end A

