  public initA: real * real ==> A
  initA  (a,b) ==
  ( r1 := a;
    r2 := b;
    return self);

  public Get: () ==> seq of real
  Get() ==
    return [r1,r2];


  public Set: real * real ==> ()
  Set(v1,v2) ==
  ( r1 := v1;
    r2 := v2 )

end A



