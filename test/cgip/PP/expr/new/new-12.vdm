class A

instance variables
  public r1 : real := 1;
  public r2: real  := 2.2;


operations

  public initA: real * real ==> A
  initA  (a,b) ==
  ( r1 := a;
    r2 := b;
    return self);

  public Get: () ==> seq of real
  Get() ==
    return [r1,r2];


  public Set: real * real ==> ()
  Set(v1,v2) ==
  ( r1 := v1;
    r2 := v2 )

end A
class C

instance variables

i : nat := 0

operations

public Set : nat ==> ()
Set(j) ==
  i := j

end C

class B

operations

Op:() ==> ()
Op() ==
  (let c = new C() in
   c.Set(1));

public Test : () ==> nat
Test() ==
  (Op();
   return 1)

end B
