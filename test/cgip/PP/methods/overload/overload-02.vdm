-- Test case which checks that overlapping types in operation parameters 
-- are detected

class A

operations

op : real ==> nat
op(-) ==
  return 42;

op : [BP] * set of BP ==> nat
op(-,-) ==
  return 8;

op : [set of BP] * (set of BP | seq of nat) ==> nat
op(-,-) ==
  return 9;

public Test: () ==> nat
Test() ==
  op(99999);

end A
