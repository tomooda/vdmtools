class MATH

-- 	IFAD VDMTools STANDARD LIBRARY: MATH
--      --------------------------------------------
-- 
-- Standard library for the VDMTools Interpreter. When the interpreter
-- evaluates the preliminary functions/operations in this file,
-- corresponding internal functions is called instead of issuing a run
-- time error. Signatures should not be changed, as well as name of
-- module (IFAD VDM-SL) or class (VDM++). Pre/post conditions is 
-- fully user customisable. 

  operations

    sin:real ==> real
    sin(-) ==
    is not yet specified    
    post abs RESULT <= 1;

    cos:real ==> real
    cos(-) ==
    is not yet specified
    post abs RESULT <= 1;

    tan:real ==> real
    tan(-) ==
    is not yet specified;

    cot:real ==> real	
    cot(-) ==
    is not yet specified; -- Could also be: return 1/tan(r)

    asin:real ==> real
    asin(a) ==
    is not yet specified
    pre abs a <= 1;

    acos:real ==> real
    acos(a) ==
    is not yet specified
    pre abs a <= 1;

    atan:real ==> real
    atan(-) ==
    is not yet specified;

    sqrt:real ==> real
    sqrt(a) ==
    is not yet specified
    pre a >= 0;

    pi_f:() ==> real
    pi_f () ==
      is not yet specified

  values
    pi = 3.14159265358979323846

 
end MATH
