class NumExpTest
  functions

  f : nat -> nat
  f(n) ==
    n + 1;

  g: nat -> bool
  g(n) ==
   n = 9;

  h: nat * nat +> nat
  h(n1,n2) ==
    n1 + n2;

  i: () -> nat
  i() ==
    8
    


------------------------------ Actual Test data ------------------------------
operations

TestOP: () ==> ()
TestOP() ==
  let - = (if 5 = 9 then {8 |-> 9} else g) comp (if 8 <> 5 then f else {9 |-> 4})
  in skip;

------------------------------------------------------------------------------
end NumExpTest
