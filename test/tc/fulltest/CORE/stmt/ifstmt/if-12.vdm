class IfStmtTest

operations

  If12: seq of bool ==> set of int
  If12(sb) ==
    if true
    then for a in sb do
           return not sb
    elseif 1>3
    then for a in reverse sb do
           return sb
    elseif 2>3
    then for all a in set sb do
           skip
    elseif 3>3
    then for i=1 to 4 by 2 do
           return sb(i) and true
    elseif 4>3
    then while true do
           return
    else let n = 7 in return n;

end IfStmtTest
