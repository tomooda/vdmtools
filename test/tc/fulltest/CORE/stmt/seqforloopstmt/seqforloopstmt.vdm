class SeqForLoopTest

operations

  SeqForLoop1: () ==> nat1
  SeqForLoop1() ==
    for a in [1,4,9] do
      return a;

  SeqForLoop2: seq of nat ==> bool
  SeqForLoop2(arg) ==
    for mk_(a,-) in arg do
      let n = 5 in return a and n;

  SeqForLoop3: bool ==> ()
  SeqForLoop3(b) ==
    let n = 7 in
      for a in if b
             then [1,6,9]
	     else n
    do
      skip;

  SeqForLoop4: () ==> nat1
  SeqForLoop4() ==
    for a in [] do
      return a;

  SeqForLoop5: () ==> ()
  SeqForLoop5() ==
    for a in 7*9 do
      return a;

  SeqForLoop6: () ==> nat1
  SeqForLoop6() ==
    for a in set {1,2} in [1,3,8] do
      return a
      
end SeqForLoopTest

