class A

functions
protected fn1(-: int) r: int ==
  1
post r = 1;

protected fn2(a: int, b: int) r: bool ==
  a = b
post r = (a = b);

protected fn3(a: int) r: bool ==
  a >= 0
post r = (a >= 0);

end A

class B is subclass of A

functions
fn1(-: int) r: int ==
  1
post r = 1;

fn2(a: char, b: char) r: bool ==
  a <> b
post r = (a <> b);

fn3(-: int) r: char ==
  'a'
post r = 'a';

end B
